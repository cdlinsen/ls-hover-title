{
    //"extends": "prettier",
    "parser": "babel-eslint",
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    },
    "plugins": [
        "react",
        "import"
    ],
    "env": {
        "amd": true,
        "browser": true,
        "jquery": true,
        "node": true,
        "es6": true,
        "worker": true
    },
    "rules": {
        "no-console": [
            1,
            {
                "allow": [
                    "error",
                    "info",
                    "warn"
                ]
            }
        ],
        "no-debugger": 1,
        "no-empty": 1,
        "no-unused-vars": [
            1,
            {
                "args": "none"
            }
        ],
        "no-undef": 2,
        "no-unreachable": 1,
        "no-use-before-define": 2,
        "react/jsx-key": 1,
        "react/jsx-no-duplicate-props": 1,
        "react/jsx-no-undef": 2,
        "react/jsx-uses-vars": 1
    }
}
